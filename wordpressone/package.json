{
  "name": "wpsass",
  "version": "1.0.0",
  "description": "Use Sass with WordPress, Node Script in SCSS flavour.",
  "scripts": {
    "css": "node-sass --output-style compressed --include-path scss sass/style.scss style.css",
    "watch": "nodemon -e scss -x \"npm run css\""
  },
  "homepage": "https://github.com/ahmadawais/WPSass#readme",
  "devDependencies": {
    "browser-sync": "^2.24.7",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {}
}
